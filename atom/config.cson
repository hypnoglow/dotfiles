"*":
  Zen:
    gutter: true
    width: 160
  "autocomplete-go": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    fileBlacklist: [
      "*.md"
      "*.txt"
    ]
    minimumWordLength: 2
  core:
    autoHideMenuBar: true
    automaticallyUpdate: false
    closeEmptyWindows: false
    disabledPackages: [
      "emmet"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".tags"
    ]
    packagesWithKeymapsDisabled: [
      "fuzzy-finder"
      "go-debug"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-light-ui"
      "atom-light-syntax"
    ]
    warnOnLargeFileLimit: 8
  editor:
    autoIndentOnPaste: false
    backUpBeforeSaving: true
    fontFamily: "Fira Code"
    invisibles: {}
    lineHeight: 1.25
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    tabLength: 4
  "exception-reporting":
    userId: "e64440d0-5c7e-4fd3-a5db-5b7c0a25c6e7"
  "expand-region": {}
  fonts:
    fontFamily: "Iosevka"
    preview: "[Hello, world!] {Привет, как дела?}"
  "fuzzy-finder": {}
  "go-guru": {}
  "go-plus":
    panelDisplayMode: "closed"
  gofmt: {}
  "gometalinter-linter":
    args: [
      "--concurrency=1"
      "--deadline=30s"
      "--vendor"
      "--disable-all"
      "--enable=vet"
      "--enable=vetshadow"
      "--enable=golint"
      "--enable=ineffassign"
      "--enable=goconst"
      "--tests"
      "--json"
      "."
    ]
  "js-hyperclick": {}
  "language-archlinux":
    checkPKGBUILDOnSave: true
  linter: {}
  "linter-eslint":
    fixOnSave: true
  "linter-jshint": {}
  "linter-php": {}
  "linter-rust": {}
  "linter-shellcheck":
    userParameters: "-e SC1090,SC2164"
  "linter-ui-default":
    showPanel: true
  minimap:
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
  "one-dark-ui": {}
  "package-generator":
    packageSyntax: "coffeescript"
  pigments: {}
  "rust-api-docs-helper":
    enableBackgroundResolving: false
  scrolloff:
    verticalMargin: 8
  "seti-syntax": {}
  "seti-ui": {}
  "split-diff": {}
  "tester-go":
    coverageHighlightMode: "covered-and-uncovered"
  "tree-view": {}
  welcome:
    showOnStartup: false
".coffee.source":
  editor:
    preferredLineLength: 80
    tabLength: 4
".dots.source":
  editor:
    preferredLineLength: 80
".go.source":
  editor:
    tabLength: 4
".gohtml.html.text":
  editor:
    tabLength: 4
".gotemplate.source":
  editor:
    tabLength: 4
".html.js.text":
  editor:
    tabLength: 2
".js.regexp.replacement.source":
  editor:
    tabLength: 4
".js.regexp.source":
  editor:
    tabLength: 4
".js.source":
  editor:
    tabLength: 4
".litcoffee.source":
  editor:
    tabLength: 4
".raml.source":
  editor:
    tabLength: 4
".shell-session.text":
  editor:
    tabLength: 4
".shell.source":
  editor:
    tabLength: 4
".source.tmux":
  editor:
    tabLength: 4
".text.xml":
  editor:
    tabLength: 2
".text.xml.xsl":
  editor:
    tabLength: 2
