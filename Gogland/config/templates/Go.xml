<templateSet group="Go">
  <template name="fpd" value="fmt.Printf(&quot;DEBUG: %#v\n&quot;, $ITEM$)&#10;os.Exit(1)" description="Printf debug" toReformat="false" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="fpe" value="fmt.Println(&quot;@@@ ERROR @@@ : &quot;, err)" description="Print error" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="fpl" value="fmt.Println($ITEM$)" description="fmt.Println" toReformat="false" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="fef" value="fmt.Errorf($VALUE$, $END$)" description="fmt.Errorf()" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="lfl" value="log.Fatalln($ITEM$)" description="log.Fatalln()" toReformat="false" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="lpf" value="log.Printf($ITEM$)" description="log.Printf()" toReformat="true" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="lfe" value="log.Fatalln(err)" description="log.Fatalln(err)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="lfn" value="log.Fatalln($END$)" description="log.Fatalln()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="h200" value="http.StatusOK" description="http.StatusOK" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="hte" value="http.Error(w, $MESSAGE$, $CODE$)" description="http.Error(w, err.Error(), http.StatusInternalServerError)" toReformat="false" toShortenFQNames="true">
    <variable name="MESSAGE" expression="" defaultValue="&quot;err.Error()&quot;" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="&quot;http.StatusInternalServerError&quot;" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="hhf" value="http.HandleFunc($PATH$, func(w http.ResponseWriter, r *http.Request) {&#10;    $END$&#10;})" description="http.HandleFunc" toReformat="false" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="&quot;\&quot;/\&quot;&quot;" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="hf" value="func $NAME$(w http.ResponseWriter, r *http.Request) {&#10;    $END$&#10;}" description="func (w http.ResponseWriter, r *http.Request)" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="dfc" value="defer func() {&#10;    cerr := $PARAM$.Close()&#10;    if err == nil {&#10;        err = cerr&#10;    }&#10;}()&#10;$END$" description="defer Close()" toReformat="false" toShortenFQNames="true">
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
</templateSet>
