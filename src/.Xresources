! URxvt.font: 9x15
! URxvt.font: xft:Terminus:size=12
! URxvt.font: xft:Source Code Pro:Medium:pixelsize=12:antialias=true:hinting=true
! xft:Iosevka Term:size=12:antialias=true:hinting=true

! Font settings
URxvt.font: -misc-tamzen-medium-r-normal--16-116-100-100-c-80-iso8859-1,\
            -misc-tamzenforpowerline-medium-r-normal--16-116-100-100-c-80-iso10646-1,\
            9x15
URxvt.boldFont: -misc-tamzen-bold-r-normal--16-116-100-100-c-80-iso8859-1,\
                -misc-tamzenforpowerline-bold-r-normal--16-116-100-100-c-80-iso10646-1,\
                9x15
URxvt.italicFont:
URxvt.boldItalicFont:

URxvt.letterSpace: 0
URxvt.lineSpace: 0

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xcursor.theme: Chameleon-Pearl-Regular-0.5
Xcursor.size:  0
URxvt.depth: 32
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true

!
! Scroll
!

! Scroll when output come but not when you go back in to the buffer.
! Scroll back to the end on key press
! (on program stop running and prompt is back it go back to the end too).
URxvt*saveLines:                      50000
URxvt*scrollTtyOutput:                false
URxvt*scrollWithBuffer:               true
URxvt*scrollTtyKeypress:              true

! Keybindings for 1-line scrolling
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007

!
! Urls
!

URxvt.perl-ext-common: default,matcher,url-select,selection-to-clipboard
URxvt.url-launcher: google-chrome-stable
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: google-chrome-stable
URxvt.url-select.underline: true

