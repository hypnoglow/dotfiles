"*":
  "autocomplete-plus": {}
  core:
    disabledPackages: [
      "linter-php"
      "fonts"
    ]
    themes: [
      "seti-ui"
      "seti-syntax"
    ]
  editor:
    fontFamily: "Iosevka"
    fontSize: 16
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    tabLength: 4
  "exception-reporting":
    userId: "094ce7cd-aaa4-6a3d-8328-686afda11de7"
  fonts:
    fontFamily: "Iosevka"
    preview: "[Hello, world!] {Привет, как дела?}"
  "js-hyperclick": {}
  linter:
    showErrorPanel: false
  "linter-jshint": {}
  "linter-php": {}
  "linter-rust": {}
  "linter-shellcheck": {}
  minimap:
    absoluteMode: true
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
  "one-dark-ui": {}
  "rust-api-docs-helper":
    enableBackgroundResolving: false
  "tree-view": {}
  welcome:
    showOnStartup: false
".dots.source":
  editor:
    preferredLineLength: 80
".html.js.text":
  editor:
    tabLength: 2
